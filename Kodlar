const updatedData = data.map(d => {
    const parsedDate = parseDate(d.date); // Tarih parsing işlemi
    console.log(parsedDate);  // Burada d.date'i parse ediyoruz ve sonucu yazdırıyoruz
    return {
        date: parsedDate, // Burada tarihleri dönüştürüyoruz
        value1: d.value1,
        value2: d.value2
    };
}).filter(d => d.date !== null);  // null olmayan tarihleri almak
