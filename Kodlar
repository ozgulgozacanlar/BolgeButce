 <tbody>
        {/* Verileri işleme */}
        {data.map((item, index) => (
          <tr key={index}>
            {/* İlk eleman qText'i th olarak ekle */}
            {index === 0 ? (
              <th>{item.qText}</th>
            ) : (
              <td>{item.qText}</td>
            )}
          </tr>
        ))}
      </tbody>
