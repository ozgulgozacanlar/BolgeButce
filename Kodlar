   .attr("fill", (d) => {
        // Burada d.region veya d.branch gibi bir veri kontrolü yapabilirsiniz
        if (d.type === "region") {
            return "blue"; // Bölgeye mavi renk
        } else if (d.type === "branch") {
            return "green"; // Şubeye yeşil renk
        }
        return "gray"; // Varsayılan renk
    });
